<h1 style="margin-bottom:1em;" id="transactionsHeader">Flight Transactions</h1>

<div id="tablesWrapper">
<table id="flight_reservations" class="display" data-source="<%= dashboard_transactions_flight_data_url(format: "json") %>">
  <thead>
      <tr>
        <th>ID</th>
        <th>Flight Date</th>
        <th style="width:30px;">Days Prior</th>
        <th>Airline</th>
        <th>Booked Via</th>
        <th>Online</th>
        <th>Benchmark</th>
        <th>Price</th>
        <th>Saved</th>
        <th>Percentage of savings</th>
        <th>Ticket Number</th>
        <th>Mileage</th>
        <th>Routing</th>
        <th>Domestic/International</th>
        <th>Trip Departure Date</th>
        <th>Origin Airport</th>
        <th>Destination Airport</th>
      </tr>
  </thead>
<tbody>
</tbody>
</table>


<table id="hotel_reservations" class="display" data-source="<%= dashboard_transactions_hotel_data_url(format: "json") %>">
  <thead>
      <tr>
        <th>ID</th>
        <th>CheckIn</th>
        <th>Days Prior</th>
        <th>City</th>
        <th>Hotel</th>
        <th>Online</th>
        <th>Rate</th>
        <th>#</th>
        <th>Total</th>
        <th>Savings</th>
        <th>Net Savings</th>
        <th>Percentage of savings</th>
        <th>Ticket Number</th>
        <th>Mileage</th>
        <th>Routing</th>
      </tr>
  </thead>
<tbody>
</tbody>
</table>

<table id="car_rental_reservations" class="display" data-source="<%= dashboard_transactions_car_rental_data_url(format: "json") %>">
  <thead>
      <tr>
        <th>ID</th>
        <th>Rental Date</th>
        <th>Days Prior</th>
        <th>City</th>
        <th>Agency</th>
        <th>Type</th>
        <th>Online</th>
        <th>Rate</th>
        <th>#</th>
        <th>Total</th>
        <th>Savings</th>
        <th>Ticket Number</th>
        <th>Mileage</th>
        <th>Routing</th>
        <th>Domestic/International</th>
      </tr>
  </thead>
<tbody>
</tbody>
</table>
</div>
